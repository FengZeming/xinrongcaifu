package com.xinrong.util;
/**
 * 计算器类
 * @author lenovo
 *
 */
public class Caculator {
	
	/**
	 * 计算投资收益
	 */
	public static Double profitCaculator(Integer projectduration,Double expectedannualized,Double money){
		Double time= (double)projectduration/12;
		Double aDouble=time*expectedannualized*money;
		return aDouble;
	}
}
